{{yield}}

<button type="button" {{on "click" this.log}}>console log</button>

<table>
   {{#each this.gridRows as |row|}}
   <tr>
    {{#each row as |column|}}
    <td id={{column.key}} onclick={{action this.ciao}}>
      {{column.value}}
    </td>
    {{/each}}
   </tr>
   {{/each}}
